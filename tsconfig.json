{
    "compilerOptions": {
      "noImplicitAny": false,
      "module": "ESNext",
      "target": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "noEmitOnError": true,
      "removeComments": false,
      "sourceMap": true,
      "esModuleInterop": true,
      "jsx": "react",
      "skipLibCheck": true,
      "importHelpers": false,
      "noEmitHelpers": true
    },
    "compileOnSave": false,
    "exclude": [
      "node_modules"
    ]
  }
